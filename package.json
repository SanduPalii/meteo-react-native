    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "expo": "^51.0.38",
    "expo-app-loading": "^2.1.1",
    "expo-constants": "~16.0.1",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-router": "^3.5.23",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
